@model MVCMusicStoreWithAuthentication.Models.Review

<tr>
    <td>
        @Html.DisplayFor(modelItem => Model.Album.Title)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Comment)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Rating)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = Model.ReviewId }) |
        @Html.ActionLink("Details", "Details", new { id = Model.ReviewId }) |
        @Html.ActionLink("Delete", "Delete", new { id = Model.ReviewId })
    </td>
</tr>
